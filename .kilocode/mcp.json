{
  "mcpServers": {
    "context7": {
      "command": "bunx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "API_KEY": "ctx7sk-b29879f8-1181-4a89-bfb7-f484f52f9bfd",
        "DEFAULT_MINIMUM_TOKENS": ""
      },
      "alwaysAllow": [
        "resolve-library-id",
        "get-library-docs"
      ]
    },
    "n8n-mcp": {
      "command": "bunx",
      "args": [
        "-y",
        "n8n-mcp@2.12.2"
      ],
      "env": {
        "MCP_MODE": "stdio",
        "LOG_LEVEL": "error",
        "DISABLE_CONSOLE_OUTPUT": "true"
      },
      "disabled": true,
      "alwaysAllow": []
    }
  }
}